{
    "name": "kylewlawrence/infinity-laravel",
    "type": "library",
    "description": "Laravel wrapper for kylewlawrence/infinity-api-client-php package",
    "minimum-stability": "stable",
    "keywords": [
        "infinity",
        "laravel",
        "php-sdk"
    ],
    "homepage": "http://designanddevelop.io",
    "license": "MIT",
    "authors": [
        {
            "name": "KyleWLawrence",
            "email": "Kyle@DesignAndDevelop.io"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^8.0|^9.0",
        "kylewlawrence/infinity-api-client-php": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "KyleWLawrence\\Infinity\\Providers\\InfinityServiceProvider"
            ],
            "aliases": {
                "Infinity": "KyleWLawrence\\Infinity\\Facades\\Infinity",
                "InfinityBulk": "KyleWLawrence\\Infinity\\Controllers\\InfinityBulk",
                "InfinityHelpers": "KyleWLawrence\\Infinity\\Controllers\\InfinityHelpers"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "KyleWLawrence\\Infinity\\": "src"
        },
        "files": [
            "src/Crud/Helpers.php"
        ]
    }
}
