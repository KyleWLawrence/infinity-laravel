{
    "name": "kylewlawrence/infinity-laravel",
    "type": "library",
    "description": "Laravel wrapper for kylewlawrence/infinity-api-client-php package",
    "minimum-stability": "beta",
    "keywords": [
        "infinity",
        "laravel",
        "php-sdk"
    ],
    "homepage": "http://designanddevelop.io",
    "license": "MIT",
    "authors": [
        {
            "name": "KyleWLawrence",
            "email": "Kyle@DesignAndDevelop.io"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/support": ">=8.0",
        "kylewlawrence/infinity-api-client-php": "dev-main"
    },
    "extra": {
        "laravel": {
            "providers": [
                "KyleWLawrence\\Infinity\\Providers\\InfinityServiceProvider"
            ],
            "aliases": {
                "Infinity": "KyleWLawrence\\Infinity\\Facades\\Infinity"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "KyleWLawrence\\Infinity\\": "src"
        },
        "files": [
            "src/Crud/Helpers.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:kylewlawrence/infinity-api-client-php.git"
        }
    ]
}
